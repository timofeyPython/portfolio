{
    "compilerOptions": {
      "outDir": "./dist/",
      "noImplicitAny": true,
      "module": "es6",
      "target": "es5",
      "jsx": "react",
      "allowJs": true,
      "moduleResolution": "node",
      "baseUrl": ".",
      "paths": {
        "@/*": ["src/*"],
        "@core/*": ["src/core/*"],
        "@components/*": ["src/components/*"],
        "@redux/*": ["src/redux/*"],
        "@routes/*": ["src/core/routes/*"],
        "@pages/*": ["src/pages/*"],
        "@scss/*": ["src/scss/*"],
        "@types_/*": ["src/types/*"],
      }
    },
  }